#include<stdio.h>
int main()
{
	int a[50],n,x,y,temp;
	printf("enter the number of elements in an array");
	scanf("%d",&n);
	printf("enter the elements of an array");
	for(x=0;x<n;x++)
	scanf("%d",&a[x]);
	for(x=0;x<n-1;x++)
	{	for(y=0;y<n-1;y++)
	
	{
		if(a[y]>a[y+1])
		{
			temp=a[y];
			a[y]=a[y+1];
			a[y+1]=temp;
		}}}
for(x=0;x<n;x++)
{
	printf("%d\t",a[x]);
}
return 0;
}
